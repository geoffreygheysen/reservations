{% extends 'base.html.twig' %}

{% block title %}Fiche d'un artiste{% endblock %}

{% block body %}

<h1>{{artist.firstname}} {{artist.lastname}}</h1>

<h2>Liste des types</h2>
<ul>
{% for artistType in artist.types %}
    <li>{{ artistType.type.type }}</li>
{% endfor %}
</ul>

<h2>Agent</h2>
<p>{{ artist.agent.name }}</p>

{% endblock %}